<h1>Listing seminars</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @seminars.each do |seminar| %>
  <tr>
    <td><%= seminar.name %></td>
    <td><%= link_to 'Show', seminar %></td>
    <td><%= link_to 'Edit', edit_seminar_path(seminar) %></td>
    <td><%= link_to 'Destroy', seminar, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Seminar', new_seminar_path %>







<div id="mainContent" class="grid clearfix">
    <header class="pgTitle alignLeft">
    <div class="wrapper">
          <hgroup>
            <h1><%= image_tag "ribbons/fl-left.png", {class: "ribbonLeft"} %>Free Seminars <%= image_tag "ribbons/fl-right.png", {class: "ribbonRight"} %></h1>
          </hgroup>
    </div>
    </header>
  <div class="wrapper clearfix">
     <cfinclude template="/inc/seminar-form.cfm">
  </div>
</div>
